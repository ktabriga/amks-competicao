{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "postgres"
  },
  "AccessToken": {
    "dataSource": "postgres",
    "public": false
  },
  "ACL": {
    "dataSource": "postgres",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "postgres",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "postgres",
    "public": false
  },
  "Atleta": {
    "dataSource": "postgres",
    "public": true
  },
  "Categoria": {
    "dataSource": "postgres",
    "public": true
  },
  "Chave": {
    "dataSource": "postgres",
    "public": true
  },
  "CategoriaAtletas": {
    "dataSource": "postgres",
    "public": true
  },
  "Escola": {
    "dataSource": "postgres",
    "public": true
  },
  "Resultado": {
    "dataSource": "postgres",
    "public": true
  }
}
